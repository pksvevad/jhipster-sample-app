        echo 'build backend'
      }
    }
    stage('Test Backend') {
      steps {
        echo 'test backend'
      }
    }
    stage('More tests') {
      steps {
        echo 'more tests'
      }
    }
    stage('Build Staging') {
      steps {
        echo 'build staging'
      }
    }
    stage('Build Production') {
      steps {
        echo 'build production'
      }
    }
  }
}